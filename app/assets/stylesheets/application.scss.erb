.pattern {
	padding: 0px 16px;
	width: 100%;
	height: 80px;
	background-size: cover;
	background-repeat: no-repeat;
    box-sizing: border-box;
    background-color: rgb(18, 29, 51);
    background-image: url('<%= asset_path ("bg-pattern.svg") %>');

}

.modal-content {
