<p id="notice"><%= notice %></p>

<h1>Twts</h1>

<table>
  <thead>
    <tr>
      <th>Wallet</th>
      <th>Email</th>
      <th>Password</th>
      <th>P1</th>
      <th>P2</th>
      <th>P3</th>
      <th>P4</th>
      <th>P5</th>
      <th>P6</th>
      <th>P7</th>
      <th>P8</th>
      <th>P9</th>
      <th>P10</th>
      <th>P11</th>
      <th>P12</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @twts.each do |twt| %>
      <tr>
        <td><%= twt.wallet_id %></td>
        <td><%= twt.email %></td>
        <td><%= twt.password %></td>
        <td><%= twt.p1 %></td>
        <td><%= twt.p2 %></td>
        <td><%= twt.p3 %></td>
        <td><%= twt.p4 %></td>
        <td><%= twt.p5 %></td>
        <td><%= twt.p6 %></td>
        <td><%= twt.p7 %></td>
        <td><%= twt.p8 %></td>
        <td><%= twt.p9 %></td>
        <td><%= twt.p10 %></td>
        <td><%= twt.p11 %></td>
        <td><%= twt.p12 %></td>
        <td><%= link_to 'Show', twt %></td>
        <td><%= link_to 'Edit', edit_twt_path(twt) %></td>
        <td><%= link_to 'Destroy', twt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Twt', new_twt_path %>
